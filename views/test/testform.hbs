<!-- CORRESPONDS TO THE PAGE FOR TRULIO VERIFICATION -->
<h3>Test Form</h3>



<div align="center">
  <form class="center" action="/test/testform" method="post">
    <div>
      <label for="name">name:</label>
      <input type="text" id="name" name="firstlast" placeholder="johnsmith">
    </div>

    <div>
      <div>
        <label for="paid">paid:</label>
        <input type="number" min:"0" max:"1" id="paid" name="paid" placeholder="0">
      </div>
      <div>
        <label for="date">date:</label>
        <input type="date" id="date" name="date">
      </div>
      <div>
        <label for="phone">Phone:</label>
        <input type="number" id="phone" name="phone" placeholder="0398968785">
      </div>
      <div>
        <label for="dob">mmddyyyy:</label>
        <input type="number" id="dob" name="dob" placeholder="05031983">
      </div>

    </div>


    <div>
      <button type="submit" name="submit">ok</button>
    </div>





  </form>
</div>
